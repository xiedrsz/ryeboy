// 将设置放入此文件中以覆盖默认值和用户设置。
{
  "git.confirmSync": false,
  "files.associations": {
    "*.vue": "vue"
  },
  "emmet.syntaxProfiles": {
    "vue-html": "html",
    "vue": "html"
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    {
      "language": "html",
      "autoFix": true
    },
    "vue"
  ],
  "eslint.autoFixOnSave": true,
  "editor.snippetSuggestions": "top"
}